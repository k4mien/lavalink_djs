        "secretOptions": []
      }
    },
    {
      "name": "discordjs",
      "image": "730519783440.dkr.ecr.eu-central-1.amazonaws.com/myrepo:67b2c9363dbb5ce51ddae15ec8694fa0ac134e68",
      "cpu": 0,
      "portMappings": [],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::730519783440:role/ecsTaskExecutionRole",
